{{> header}}

<div class="tags" > 
  {{#tags}}
    <div class="tag {{class}}"><a href="/?tag={{_id}}">{{_id}} ({{count}})</a></div>
  {{/tags}}
</div>

TAG: {{tag}}

{{#posts}}
    <div class="post">
      {{#photo}}
      <img  src="/media/{{photo}}" />
      {{/photo}}
      <h3> <a href="/post/{{slug}}">{{title}}</a></h3>
      <p>{{body}}</p>
      {{#user}}
      <a href="/post/{{slug}}/edit">Editar</a>
      {{/user}}
      <hr />
    </div>
{{/posts}}