{{>header}}

<h1>CADASTRO</h1>

<form method="POST">

    <label>
        Nome:
        <input type="text" name="name" />
    </label>

    <br />

    <label>
        E-mail:
        <input type="email" name="email" />
    </label>

    <br />

    <label>
        Senha: 
        <input type="password" name="password" />
    </label>

    <br />

    <label>
        <input type="submit" value="Cadastrar" />
    </label>
</form>